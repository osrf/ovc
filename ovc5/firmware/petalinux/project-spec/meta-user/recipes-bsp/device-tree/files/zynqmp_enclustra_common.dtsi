&gpio {
	status = "okay";
};

&qspi {
	is-dual = <0>;
	num-cs = <1>;
	flash@0 {
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		compatible = "s25fl512s";
		reg = <0x0>;
		spi-tx-bus-width = <0x1>;
		spi-rx-bus-width = <0x4>;
		spi-max-frequency = <0x2faf080>;
	};
};

&i2c0 {
	status = "okay";
	clock-frequency = <100000>;
	scl-gpios = <&gpio 10 GPIO_ACTIVE_HIGH>;
	sda-gpios = <&gpio 11 GPIO_ACTIVE_HIGH>;
	atsha204a@64 {
		compatible = "atmel,atsha204a";
		reg = <0x0 0x64>;
	};
};

&sdhci0 {
	status = "okay";
	non-removable;
	disable-wp;
	xlnx,mio_bank = <0>;
	bus-width = <8>;
};

&uart0 {
	status = "okay";
};

&xilinx_ams {
	status = "okay";
};

&ams_ps {
	status = "okay";
};

&ams_pl {
	status = "okay";
};
